<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>
        {% block title %}
            {{ applicationConfig['name'] }}
        {% endblock %}
    </title>

    {% block stylesheets %}
        {% include 'stylesheets.twig' %}
    {% endblock %}
</head>
<body>
<div class="container">
    {#{% include 'layouts/flash_alert.twig' %}#}

    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            {# Include common navbar brand code #}
            {% include 'navbar/navbar_brand.twig' %}

            <div id="navbar" class="navbar-collapse collapse">
                {% block navbar_links %}{% endblock %}
            </div>
        </div>
    </nav>

    {% block content %}{% endblock %}
</div>

{% block scripts %}
    {% include 'scripts.twig' %}
{% endblock %}
</body>
</html>